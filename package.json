{
  "name": "sw-inventory",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:mem": "node --max-old-space-size=4096 ./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json",
    "check:ci": "svelte-kit sync && node --max-old-space-size=4096 ./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json --output json > svelte-check.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "lucide-svelte": "^0.552.0",
    "prisma": "^6.17.1",
    "sveltekit-superforms": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
