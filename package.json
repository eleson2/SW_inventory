{
  "name": "sw-inventory",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "check:mem": "node --max-old-space-size=4096 ./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json",
    "check:ci": "svelte-kit sync && node --max-old-space-size=4096 ./node_modules/.bin/svelte-check --tsconfig ./tsconfig.json --output json > svelte-check.json",
    "migrate-prisma": "node scripts/migrate-prisma.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "test": "vitest",
    "test:unit": "vitest run src",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:integration && playwright test --reporter=html"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.16",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "lucide-svelte": "^0.552.0",
    "prisma": "^6.17.1",
    "sveltekit-superforms": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0"
  },
  "type": "module",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
